---
import About from "../components/About.astro";
import CTASection from "../components/CTASection.astro";
import FaqSection from "../components/FaqSection.astro";
import Hero from "../components/Hero.astro";
import Portfolio from "../components/Portfolio.astro";
import Services from "../components/Services.astro";
import Layout from "../layouts/Layout.astro";
import { NAV_MAIN } from "../content/menuItems";
import { HERO_MAIN } from "../content/hero";
import { PORTFOLIO_MAIN } from "../content/portfolio";
import { FAQ_MAIN } from "../content/faq";
---

<Layout nav={NAV_MAIN}>
    <Hero data={HERO_MAIN} />
    <Services />
    <About />
    <CTASection />
    <Portfolio data={PORTFOLIO_MAIN} />
    <FaqSection data={FAQ_MAIN} />
</Layout>
